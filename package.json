{
  "name": "cadence-ide",
  "version": "1.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"node server.js\" \"vite\"",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "electron": "electron .",
    "electron-dev": "concurrently \"vite --port 5173 --strictPort\" \"wait-on http://localhost:5173 && node_modules/electron/dist/Electron.app/Contents/MacOS/Electron .\"",
    "dist": "npm run build && electron-builder",
    "dist-mac": "npm run build && electron-builder --mac",
    "dist-mac-arm": "npm run build && electron-builder --mac --arm64"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lucide-react": "^0.474.0",
    "node-pty": "^1.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^25.1.8",
    "icon-gen": "^5.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^6.2.0",
    "wait-on": "^9.0.3"
  },
  "description": "cadence ide - ai powered code editor",
  "author": "beerooyay",
  "build": {
    "appId": "com.beerooyay.cadence",
    "productName": "Cadence",
    "mac": {
      "target": [
        "dir"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "build/icon.icns",
      "identity": null
    },
    "files": [
      "dist/**/*",
      "main.cjs",
      "preload.cjs",
      "cadenceserver.py"
    ],
    "asarUnpack": [
      "node_modules/node-pty/**/*"
    ]
  }
}
